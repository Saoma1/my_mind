<meta charset="utf-8" />
<meta name="viewport" content="width=device-width, initial-scale=1.0" />
<% resource_title = strip_html(strip_newlines(title)) %>
<title><% if resource_title != "Index" %><%= resource_title %> | <%= metadata.title %><% else %><%= metadata.title %>: <%= metadata.tagline %><% end %></title>

<meta name="description" content="<%= metadata.description %>" />

<%# custom attribute %>
<%# https://edge.bridgetownrb.com/docs/bundled-configurations#turbo %>
<meta name="turbo-cache-control" content="no-preview" />
<%# https://turbo.hotwired.dev/handbook/drive#reloading-when-assets-change %>
<link rel="stylesheet" href="/application-258e88d.css" data-turbo-track="reload"> 
<script src="/application-cbd3cd4.js" data-turbo-track="reload"></script>
<%# custom attribute %>

<link rel="stylesheet" href="<%= webpack_path :css %>" />
<script src="<%= webpack_path :js %>" defer></script>
<%= live_reload_dev_js %>
